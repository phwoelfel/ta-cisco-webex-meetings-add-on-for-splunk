{
    "meta": {
        "name": "TA-webex-meetings-add-on-for-splunk",
        "displayName": "WebEx Meetings Add-on for Splunk",
        "version": "1.1.0",
        "apiVersion": "3.0.0",
        "restRoot": "TA_webex_meetings_add_on_for_splunk"
    },
    "pages": {
        "configuration": {
            "title": "Configuration",
            "description": "Set up your add-on",
            "tabs": [
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "field": "loglevel",
                            "label": "Log level",
                            "type": "singleSelect",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG",
                                        "value": "DEBUG"
                                    },
                                    {
                                        "label": "INFO",
                                        "value": "INFO"
                                    },
                                    {
                                        "label": "WARNING",
                                        "value": "WARNING"
                                    },
                                    {
                                        "label": "ERROR",
                                        "value": "ERROR"
                                    },
                                    {
                                        "label": "CRITICAL",
                                        "value": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO"
                        }
                    ]
                },
                {
                    "name": "additional_parameters",
                    "title": "Add-on Settings",
                    "entity": [
                        {
                            "field": "site_name",
                            "label": "Site Name",
                            "type": "text",
                            "help": "This identifies the Webex site you are targeting with your API request. Site name is the subdomain of the Webex site URL.For example, for https://mysitename.webex.com specify mysitename",
                            "required": false,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "mysitename"
                            }
                        },
                        {
                            "field": "username",
                            "label": "Username",
                            "type": "text",
                            "help": "Service Account Username or Email address of the host or admin account making a request.",
                            "required": true,
                            "defaultValue": "",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "localuser or user@example.com"
                            }
                        },
                        {
                            "field": "password",
                            "label": "Password",
                            "type": "text",
                            "help": "Service Account Password",
                            "required": true,
                            "defaultValue": "",
                            "encrypted": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of password is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "*  *  *"
                            }
                        }
                    ]
                }
            ]
        },
        "inputs": {
            "title": "Inputs",
            "description": "Manage your data inputs",
            "table": {
                "header": [
                    {
                        "field": "name",
                        "label": "Name"
                    },
                    {
                        "field": "interval",
                        "label": "Interval"
                    },
                    {
                        "field": "index",
                        "label": "Index"
                    },
                    {
                        "field": "disabled",
                        "label": "Status"
                    }
                ],
                "moreInfo": [
                    {
                        "field": "name",
                        "label": "Name"
                    },
                    {
                        "field": "interval",
                        "label": "Interval"
                    },
                    {
                        "field": "index",
                        "label": "Index"
                    },
                    {
                        "field": "disabled",
                        "label": "Status"
                    },
                    {
                        "field": "live",
                        "label": "Monitor Active Session"
                    },
                    {
                        "field": "endpoints",
                        "label": "Endpoints"
                    },
                    {
                        "field": "start_time_start",
                        "label": "Begin Time"
                    }
                ],
                "actions": [
                    "edit",
                    "enable",
                    "delete",
                    "clone"
                ]
            },
            "services": [
                {
                    "name": "meetings_general_service",
                    "title": "General Service",
                    "entity": [
                        {
                            "field": "name",
                            "label": "Name",
                            "type": "text",
                            "help": "Enter a unique name for the data input",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^[a-zA-Z]\\w*$",
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores."
                                },
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 100,
                                    "errorMsg": "Length of input name should be between 1 and 100"
                                }
                            ]
                        },
                        {
                            "field": "interval",
                            "label": "Interval",
                            "type": "text",
                            "required": true,
                            "help": "Time interval of input in seconds.",
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$",
                                    "errorMsg": "Interval must be an integer."
                                }
                            ]
                        },
                        {
                            "field": "index",
                            "label": "Index",
                            "type": "singleSelect",
                            "defaultValue": "default",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "blackList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 80,
                                    "errorMsg": "Length of index name should be between 1 and 80."
                                }
                            ]
                        },
                        {
                            "field": "live",
                            "label": "Monitor Active Session",
                            "help": "Use this to retrieve Active Session Data such as information for all service types, including Meeting Center, Training Center, Event Center, Sales Center and Support Center.",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        }
                    ]
                },
                {
                    "name": "meetings_history_service",
                    "title": "History Service",
                    "entity": [
                        {
                            "field": "name",
                            "label": "Name",
                            "type": "text",
                            "help": "Enter a unique name for the data input",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^[a-zA-Z]\\w*$",
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores."
                                },
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 100,
                                    "errorMsg": "Length of input name should be between 1 and 100"
                                }
                            ]
                        },
                        {
                            "field": "interval",
                            "label": "Interval",
                            "type": "text",
                            "required": true,
                            "help": "Time interval of input in seconds.",
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$",
                                    "errorMsg": "Interval must be an integer."
                                }
                            ]
                        },
                        {
                            "field": "index",
                            "label": "Index",
                            "type": "singleSelect",
                            "defaultValue": "default",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "blackList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 80,
                                    "errorMsg": "Length of index name should be between 1 and 80."
                                }
                            ]
                        },
                        {
                            "field": "endpoints",
                            "label": "Endpoints",
                            "help": "Please choose the historical endpoints that are used to fetch data back. Note: Please specify a Begin Time below and do not check the Continuous Monitoring when retrieving historical data.",
                            "required": true,
                            "type": "multipleSelect",
                            "options": {
                                "delimiter": "~",
                                "items": [
                                    {
                                        "value": "LstmeetingusageHistory",
                                        "label": "Meeting Usage"
                                    },
                                    {
                                        "value": "LstsupportsessionHistory",
                                        "label": "Support Sessions"
                                    },
                                    {
                                        "value": "LsttrainingsessionHistory",
                                        "label": "Training Sessions"
                                    },
                                    {
                                        "value": "LsteventsessionHistory",
                                        "label": "Event Session"
                                    },
                                    {
                                        "value": "LstrecordaccessHistory",
                                        "label": "Recorded Access"
                                    },
                                    {
                                        "value": "LstmeetingattendeeHistory",
                                        "label": "Meeting Attendees"
                                    }
                                ]
                            }
                        },
                        {
                            "field": "start_time_start",
                            "label": "Begin Time",
                            "help": "This is the time from where you want to ingest the historical data.  Please enter UTC time. Format: MM/DD/YYYY hh:mm:ss\n\nNOTE: Begin Date must be at least 3 days ago and ideally no more than 90 days.",
                            "required": true,
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "04/10/2020 10:00:00"
                            }
                        }
                    ]
                }
            ]
        }
    }
}